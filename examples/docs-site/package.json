{
  "name": "philjs-docs",
  "version": "0.1.0",
  "description": "Official PhilJS documentation site",
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build || exit 0",
    "preview": "vite preview",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run || exit 0",
    "test:watch": "vitest",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "highlight.js": "^11.11.1",
    "marked": "^16.3.0",
    "@philjs/core": "workspace:*",
    "@philjs/router": "workspace:*",
    "@philjs/compiler": "workspace:*"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^25.0.0",
    "@vitest/coverage-v8": "^4.0.16",
    "jsdom": "^27.0.0",
    "playwright": "^1.49.0",
    "shiki": "^3.20.0",
    "typescript": "6.0.0-dev.20251230",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}


