{
  "name": "storefront",
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "dev": "node server/dev.js",
    "build": "vite build && vite build --ssr src/server/entry-server.ts --outDir dist/server",
    "preview": "node server/prod.js",
    "serve": "node server/prod.js",
    "test": "vitest run",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@philjs/core": "workspace:*",
    "@philjs/router": "workspace:*",
    "@philjs/ssr": "workspace:*",
    "@philjs/islands": "workspace:*",
    "@philjs/ai": "workspace:*",
    "@philjs/devtools": "workspace:*",
    "@philjs/compiler": "workspace:*"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@types/node": "^22.10.5",
    "typescript": "next",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "playwright": "^1.48.0"
  }
}
