{
  "name": "create-philjs",
  "version": "0.1.0",
  "type": "module",
  "description": "Create PhilJS apps with one command",
  "bin": {
    "create-philjs": "./dist/cli.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup src/cli.ts --format esm --dts",
    "dev": "tsup src/cli.ts --watch"
  },
  "keywords": [
    "philjs",
    "create",
    "cli",
    "scaffold",
    "rust",
    "wasm"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "dependencies": {
    "@philjs/router": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^14.0.2",
    "enquirer": "^2.4.1",
    "execa": "^9.6.1",
    "fs-extra": "^11.2.0",
    "gradient-string": "^2.0.2",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.0.0",
    "tsup": "^8.5.1",
    "typescript": "^6.0.0"
  },
  "engines": {
    "node": ">=24"
  }
}
