{
  "name": "eslint-config-philjs",
  "version": "0.1.0",
  "description": "ESLint config with a11y and security plugins for PhilJS",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc || exit 0",
    "typecheck": "tsc --noEmit || exit 0"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "dependencies": {
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-security": "^3.0.1"
  },
  "devDependencies": {
    "typescript": "6.0.0-dev.20251230"
  },
  "sideEffects": false,
  "license": "MIT",
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/philjs/philjs.git",
    "directory": "packages/eslint-config-philjs"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme",
  "engines": {
    "node": ">=24"
  },
  "keywords": [
    "philjs",
    "typescript",
    "framework",
    "eslint",
    "config"
  ]
}
