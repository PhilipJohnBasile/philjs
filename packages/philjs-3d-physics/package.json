{
  "name": "@philjs/3d-physics",
  "version": "0.1.0",
  "type": "module",
  "description": "High-performance 3D physics engine for PhilJS - rigid bodies, vehicles, ragdolls, character controllers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "lint": "eslint src/",
    "test": "vitest"
  },
  "keywords": ["philjs", "physics", "3d", "rapier", "rigid-body", "vehicle", "ragdoll", "game"],
  "author": "PhilJS Team",
  "license": "MIT",
  "peerDependencies": {
    "@philjs/core": "workspace:*",
    "@dimforge/rapier3d": ">=0.11.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=24"
  }
}
