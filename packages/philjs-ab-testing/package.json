{
  "name": "@philjs/ab-testing",
  "version": "0.1.0",
  "type": "module",
  "description": "Native A/B testing framework for PhilJS - experiments, feature flags, statistical analysis",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "lint": "eslint src/",
    "test": "vitest"
  },
  "keywords": [
    "philjs",
    "ab-testing",
    "experiments",
    "feature-flags",
    "statistical-analysis"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "peerDependencies": {
    "@philjs/core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^6.0.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=24"
  }
}
