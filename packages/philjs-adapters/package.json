{
  "name": "philjs-adapters",
  "version": "2.0.0",
  "description": "Deployment adapters for PhilJS - Vercel, Netlify, Cloudflare, AWS, Bun, Deno",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vercel": {
      "types": "./dist/vercel/index.d.ts",
      "import": "./dist/vercel/index.js"
    },
    "./netlify": {
      "types": "./dist/netlify/index.d.ts",
      "import": "./dist/netlify/index.js"
    },
    "./cloudflare": {
      "types": "./dist/cloudflare/index.d.ts",
      "import": "./dist/cloudflare/index.js"
    },
    "./aws": {
      "types": "./dist/aws/index.d.ts",
      "import": "./dist/aws/index.js"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/index.js"
    },
    "./static": {
      "types": "./dist/static/index.d.ts",
      "import": "./dist/static/index.js"
    },
    "./bun": {
      "types": "./dist/bun/index.d.ts",
      "bun": "./dist/bun/index.js",
      "import": "./dist/bun/index.js"
    },
    "./deno": {
      "types": "./dist/deno/index.d.ts",
      "deno": "./dist/deno/index.js",
      "import": "./dist/deno/index.js"
    },
    "./runtime-detect": {
      "types": "./dist/runtime-detect.d.ts",
      "import": "./dist/runtime-detect.js"
    },
    "./cloudflare-pages": {
      "types": "./dist/cloudflare-pages/index.d.ts",
      "import": "./dist/cloudflare-pages/index.js"
    },
    "./vercel/adapter": {
      "types": "./dist/vercel/adapter.d.ts",
      "import": "./dist/vercel/adapter.js"
    },
    "./netlify/adapter": {
      "types": "./dist/netlify/adapter.d.ts",
      "import": "./dist/netlify/adapter.js"
    },
    "./aws-lambda": {
      "types": "./dist/aws-lambda/index.d.ts",
      "import": "./dist/aws-lambda/index.js"
    },
    "./railway": {
      "types": "./dist/railway/index.d.ts",
      "import": "./dist/railway/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Lint philjs-adapters'"
  },
  "dependencies": {
    "philjs-core": "workspace:*",
    "philjs-ssr": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.0.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@vercel/node": "^5.5.15",
    "@netlify/functions": "^2.0.0",
    "@cloudflare/workers-types": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@vercel/node": {
      "optional": true
    },
    "@netlify/functions": {
      "optional": true
    },
    "@cloudflare/workers-types": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "philjs",
    "adapter",
    "vercel",
    "netlify",
    "cloudflare",
    "aws",
    "bun",
    "deno",
    "deployment",
    "edge",
    "serverless"
  ],
  "license": "MIT",
  "sideEffects": false,
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/philjs.git",
    "directory": "packages/philjs-adapters"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme"
}
