{
  "name": "@philjs/adapters",
  "version": "0.0.1",
  "description": "Deployment adapters for PhilJS - Vercel, Netlify, Cloudflare, AWS, Bun, Deno, and more",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vercel": {
      "types": "./dist/vercel/index.d.ts",
      "import": "./dist/vercel/index.js"
    },
    "./netlify": {
      "types": "./dist/netlify/index.d.ts",
      "import": "./dist/netlify/index.js"
    },
    "./cloudflare": {
      "types": "./dist/cloudflare/index.d.ts",
      "import": "./dist/cloudflare/index.js"
    },
    "./aws": {
      "types": "./dist/aws/index.d.ts",
      "import": "./dist/aws/index.js"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/index.js"
    },
    "./static": {
      "types": "./dist/static/index.d.ts",
      "import": "./dist/static/index.js"
    },
    "./bun": {
      "types": "./dist/bun/index.d.ts",
      "bun": "./dist/bun/index.js",
      "import": "./dist/bun/index.js"
    },
    "./deno": {
      "types": "./dist/deno/index.d.ts",
      "deno": "./dist/deno/index.js",
      "import": "./dist/deno/index.js"
    },
    "./runtime-detect": {
      "types": "./dist/runtime-detect.d.ts",
      "import": "./dist/runtime-detect.js"
    },
    "./cloudflare-pages": {
      "types": "./dist/cloudflare-pages/index.d.ts",
      "import": "./dist/cloudflare-pages/index.js"
    },
    "./vercel/adapter": {
      "types": "./dist/vercel/adapter.d.ts",
      "import": "./dist/vercel/adapter.js"
    },
    "./netlify/adapter": {
      "types": "./dist/netlify/adapter.d.ts",
      "import": "./dist/netlify/adapter.js"
    },
    "./aws-lambda": {
      "types": "./dist/aws-lambda/index.d.ts",
      "import": "./dist/aws-lambda/index.js"
    },
    "./railway": {
      "types": "./dist/railway/index.d.ts",
      "import": "./dist/railway/index.js"
    },
    "./edge": {
      "types": "./dist/edge/index.d.ts",
      "import": "./dist/edge/index.js"
    },
    "./adapters/vercel": {
      "types": "./dist/adapters/vercel.d.ts",
      "import": "./dist/adapters/vercel.js"
    },
    "./adapters/cloudflare": {
      "types": "./dist/adapters/cloudflare.d.ts",
      "import": "./dist/adapters/cloudflare.js"
    },
    "./adapters/deno-deploy": {
      "types": "./dist/adapters/deno-deploy.d.ts",
      "import": "./dist/adapters/deno-deploy.js"
    },
    "./adapters/netlify": {
      "types": "./dist/adapters/netlify.d.ts",
      "import": "./dist/adapters/netlify.js"
    },
    "./adapters/aws-lambda": {
      "types": "./dist/adapters/aws-lambda.d.ts",
      "import": "./dist/adapters/aws-lambda.js"
    },
    "./adapters/node": {
      "types": "./dist/adapters/node.d.ts",
      "import": "./dist/adapters/node.js"
    },
    "./adapters/bun": {
      "types": "./dist/adapters/bun.d.ts",
      "import": "./dist/adapters/bun.js"
    },
    "./utils/build": {
      "types": "./dist/utils/build.d.ts",
      "import": "./dist/utils/build.js"
    },
    "./utils/env": {
      "types": "./dist/utils/env.d.ts",
      "import": "./dist/utils/env.js"
    }
  },
  "scripts": {
    "build": "(tsc --noEmitOnError false || true)",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Lint philjs-adapters'"
  },
  "dependencies": {
    "@philjs/core": "workspace:*",
    "@philjs/ssr": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.21",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@vercel/node": "^5.5.15",
    "@netlify/functions": "^2.0.0",
    "@netlify/edge-functions": "^2.0.0",
    "@cloudflare/workers-types": "^4.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-lambda": "^3.0.0",
    "@aws-sdk/client-cloudfront": "^3.0.0",
    "express": "^4.18.0",
    "fastify": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@vercel/node": {
      "optional": true
    },
    "@netlify/functions": {
      "optional": true
    },
    "@netlify/edge-functions": {
      "optional": true
    },
    "@cloudflare/workers-types": {
      "optional": true
    },
    "@aws-sdk/client-s3": {
      "optional": true
    },
    "@aws-sdk/client-lambda": {
      "optional": true
    },
    "@aws-sdk/client-cloudfront": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "philjs",
    "adapter",
    "vercel",
    "netlify",
    "cloudflare",
    "aws",
    "lambda",
    "bun",
    "deno",
    "deployment",
    "edge",
    "serverless",
    "workers",
    "pages",
    "kv",
    "d1",
    "durable-objects"
  ],
  "license": "MIT",
  "sideEffects": false,
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/philjs.git",
    "directory": "packages/philjs-adapters"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme"
}
