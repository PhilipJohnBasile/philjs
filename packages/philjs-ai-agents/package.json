    "test": "vitest"
  },
  "keywords": [
    "philjs",
    "ai",
    "agents",
    "llm",
    "openai",
    "anthropic",
    "tools",
    "orchestration",
    "workflow"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "peerDependencies": {
    "@philjs/core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^6.0.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=24"
  }
}
