{
  "name": "@philjs/ai-agents",
  "version": "0.1.0",
  "type": "module",
  "description": "Multi-agent AI orchestration for PhilJS - agents, tools, memory, workflows, supervisor patterns",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
