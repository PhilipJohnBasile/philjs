{
  "name": "philjs-ai",
  "version": "0.1.0",
  "description": "AI adapter with typed prompts and safety hooks for PhilJS",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c && tsc --emitDeclarationOnly --declaration --outDir dist",
    "test": "vitest run",
    "lint": "echo 'Lint philjs-ai'",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "rollup": "^4.28.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
