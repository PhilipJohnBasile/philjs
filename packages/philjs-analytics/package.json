{
  "name": "@philjs/analytics",
  "version": "0.1.0",
  "description": "Privacy-First Analytics - GDPR/CCPA compliant, no third-party scripts, client-side processing",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./privacy-first": {
      "types": "./dist/privacy-first.d.ts",
      "default": "./dist/privacy-first.js"
    }
  },
  "scripts": {
    "build": "tsup src/privacy-first.ts --format cjs,esm --dts",
    "dev": "tsup src/privacy-first.ts --watch",
    "test": "vitest"
  },
  "keywords": [
    "philjs",
    "analytics",
    "privacy",
    "gdpr",
    "ccpa",
    "no-cookies",
    "first-party",
    "web-vitals",
    "differential-privacy"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=24"
  }
}
