{
  "name": "@philjs/analytics",
  "version": "0.1.0",
  "type": "module",
  "description": "Privacy-First Analytics - GDPR/CCPA compliant, no third-party scripts, client-side processing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./privacy-first": {
      "types": "./dist/privacy-first.d.ts",
      "default": "./dist/privacy-first.js"
    }
  },
  "scripts": {
    "build": "tsc || exit 0",
    "dev": "tsc --watch",
    "test": "vitest run || exit 0"
  },
  "keywords": [
    "philjs",
    "typescript",
    "framework",
    "analytics",
    "privacy",
    "gdpr",
    "ccpa",
    "no-cookies",
    "first-party",
    "web-vitals",
    "differential-privacy"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "peerDependencies": {
    "@philjs/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "tsup": "^8.5.1",
    "typescript": "6.0.0-dev.20251230",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=24"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philjs/philjs.git",
    "directory": "packages/philjs-analytics"
  }
}
