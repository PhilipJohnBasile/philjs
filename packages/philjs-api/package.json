{
  "name": "philjs-api",
  "version": "2.0.0",
  "description": "API routes and full-stack utilities for PhilJS",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./server": "./dist/server.js",
    "./client": "./dist/client.js",
    "./cookies": "./dist/cookies.js",
    "./session": "./dist/session.js",
    "./flash": "./dist/flash.js",
    "./cookie-session": "./dist/cookie-session.js",
    "./session-utils": "./dist/session-utils.js",
    "./edge-middleware": "./dist/edge-middleware.js",
    "./geolocation": "./dist/geolocation.js",
