{
  "name": "philjs-auth",
  "version": "0.0.1",
  "description": "Authentication and authorization utilities for PhilJS",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./oauth": {
      "types": "./dist/oauth.d.ts",
      "import": "./dist/oauth.js"
    },
    "./jwt": {
      "types": "./dist/jwt.d.ts",
      "import": "./dist/jwt.js"
    },
    "./session": {
      "types": "./dist/session.d.ts",
      "import": "./dist/session.js"
    },
    "./session-refresh": {
      "types": "./dist/session-refresh.d.ts",
      "import": "./dist/session-refresh.js"
    },
    "./protected-route": {
      "types": "./dist/protected-route.d.ts",
      "import": "./dist/protected-route.js"
    },
    "./protected-routes": {
      "types": "./dist/protected-routes.d.ts",
      "import": "./dist/protected-routes.js"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "import": "./dist/hooks.js"
    },
    "./auth-provider": {
      "types": "./dist/auth-provider.d.ts",
      "import": "./dist/auth-provider.js"
    },
    "./providers/clerk": {
      "types": "./dist/providers/clerk.d.ts",
      "import": "./dist/providers/clerk.js"
    },
    "./providers/auth0": {
      "types": "./dist/providers/auth0.d.ts",
      "import": "./dist/providers/auth0.js"
    },
    "./providers/supabase": {
      "types": "./dist/providers/supabase.d.ts",
      "import": "./dist/providers/supabase.js"
    },
    "./providers/next-auth": {
      "types": "./dist/providers/next-auth.d.ts",
      "import": "./dist/providers/next-auth.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "lint": "echo 'Lint philjs-auth'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "philjs-core": "workspace:*",
    "philjs-router": "workspace:*"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "rollup": "^4.53.3",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "philjs",
    "auth",
    "authentication",
    "oauth",
    "jwt",
    "session"
  ],
  "license": "MIT",
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/philjs.git",
    "directory": "packages/philjs-auth"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme"
}
