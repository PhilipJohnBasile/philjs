{
  "name": "@philjs/cdn",
  "version": "0.1.0",
  "description": "PhilJS CDN bundle - single file for Alpine.js-style usage, no build step required",
  "type": "module",
  "main": "./dist/philjs.global.js",
  "module": "./dist/philjs.esm.js",
  "unpkg": "./dist/philjs.global.js",
  "jsdelivr": "./dist/philjs.global.js",
  "exports": {
    ".": {
      "import": "./dist/philjs.esm.js",
      "script": "./dist/philjs.global.js"
    },
    "./global": "./dist/philjs.global.js",
    "./esm": "./dist/philjs.esm.js",
    "./mini": "./dist/philjs.mini.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsx build.ts",
    "build:watch": "tsx build.ts --watch"
  },
  "devDependencies": {
    "esbuild": "^0.27.2",
    "terser": "^5.26.0",
    "tsx": "^4.19.1",
    "typescript": "6.0.0-dev.20251230"
  },
  "keywords": [
    "philjs",
    "typescript",
    "framework",
    "cdn",
    "umd",
    "global",
    "no-build",
    "alpine-style"
  ],
  "license": "MIT",
  "sideEffects": false,
  "engines": {
    "node": ">=24"
  },
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/philjs/philjs.git",
    "directory": "packages/philjs-cdn"
  }
}
