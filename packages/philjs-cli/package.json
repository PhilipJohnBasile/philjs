{
  "name": "@philjs/cli",
  "version": "0.1.0",
  "description": "CLI tools for PhilJS - The framework that thinks ahead",
  "type": "module",
  "bin": {
    "philjs": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "dev": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript -w",
    "test": "vitest run"
  },
  "keywords": [
    "philjs",
    "cli",
    "framework",
    "build-tool",
    "dev-server"
  ],
  "dependencies": {
    "@philjs/core": "workspace:*",
    "@philjs/router": "workspace:*",
    "@philjs/ssr": "workspace:*",
    "vite": "^7.3.0",
    "rollup": "^4.54.0",
    "esbuild": "^0.27.2",
    "chokidar": "^5.0.0",
    "picocolors": "^1.1.1",
    "commander": "^14.0.2",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "typescript": "6.0.0-dev.20251230",
    "vitest": "^4.0.16",
    "@types/node": "^25.0.0",
    "@types/prompts": "^2.4.9"
  },
  "sideEffects": true,
  "license": "MIT",
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/philjs.git",
    "directory": "packages/philjs-cli"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme",
  "engines": {
    "node": ">=24"
  }
}

