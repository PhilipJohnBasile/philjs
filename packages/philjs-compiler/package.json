{
  "name": "philjs-compiler",
  "version": "0.1.0",
  "description": "Automatic optimization compiler for PhilJS - zero-overhead memoization and performance optimizations",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "import": "./dist/plugins/vite.js",
      "types": "./dist/plugins/vite.d.ts"
    },
    "./rollup": {
      "import": "./dist/plugins/rollup.js",
      "types": "./dist/plugins/rollup.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/core": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/generator": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@rollup/pluginutils": "^5.0.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/babel__traverse": "^7.20.0",
    "@types/babel__generator": "^7.6.0",
    "philjs-core": "workspace:*",
    "rollup": "^4.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4",
    "vite": "^6.4.1"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0",
    "rollup": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "vite": { "optional": true },
    "rollup": { "optional": true }
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "philjs",
    "compiler",
    "optimization",
    "memoization",
    "signals",
    "reactive"
  ],
  "author": "",
  "license": "MIT"
}
