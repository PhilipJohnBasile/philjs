{
  "name": "philjs-core",
  "version": "0.1.0",
  "description": "Core signals, memos, and resources for PhilJS",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./signals": {
      "types": "./dist/signals.d.ts",
      "import": "./dist/signals.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.js"
    },
    "./accessibility": {
      "types": "./dist/accessibility.d.ts",
      "import": "./dist/accessibility.js"
    },
    "./ab-testing": {
      "types": "./dist/ab-testing.d.ts",
      "import": "./dist/ab-testing.js"
    },
    "./forms": {
      "types": "./dist/forms.d.ts",
      "import": "./dist/forms.js"
    },
    "./i18n": {
      "types": "./dist/i18n.d.ts",
      "import": "./dist/i18n.js"
    },
    "./animation": {
      "types": "./dist/animation.d.ts",
      "import": "./dist/animation.js"
    },
    "./ppr": {
      "types": "./dist/ppr.d.ts",
      "import": "./dist/ppr.js"
    },
    "./activity": {
      "types": "./dist/activity.d.ts",
      "import": "./dist/activity.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "lint": "echo 'Lint philjs-core'",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^27.0.0",
    "rollup": "^4.28.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
