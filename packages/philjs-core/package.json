{
  "name": "philjs-core",
  "version": "0.1.0",
  "description": "Core signals, memos, and resources for PhilJS",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./signals": {
      "types": "./dist/signals.d.ts",
      "import": "./dist/signals.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.js"
    },
    "./render-to-string": {
      "types": "./dist/render-to-string.d.ts",
      "import": "./dist/render-to-string.js"
    },
    "./hydrate": {
      "types": "./dist/hydrate.d.ts",
      "import": "./dist/hydrate.js"
    },
    "./context": {
      "types": "./dist/context.d.ts",
      "import": "./dist/context.js"
    },
    "./error-boundary": {
      "types": "./dist/error-boundary.d.ts",
      "import": "./dist/error-boundary.js"
    },
    "./forms": {
      "types": "./dist/forms.d.ts",
      "import": "./dist/forms.js"
    },
    "./i18n": {
      "types": "./dist/i18n.d.ts",
      "import": "./dist/i18n.js"
    },
    "./animation": {
      "types": "./dist/animation.d.ts",
      "import": "./dist/animation.js"
    },
    "./accessibility": {
      "types": "./dist/accessibility.d.ts",
      "import": "./dist/accessibility.js"
    },
    "./ab-testing": {
      "types": "./dist/ab-testing.d.ts",
      "import": "./dist/ab-testing.js"
    },
    "./result": {
      "types": "./dist/result.d.ts",
      "import": "./dist/result.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./resumability": {
      "types": "./dist/resumability.d.ts",
      "import": "./dist/resumability.js"
    },
    "./data-layer": {
      "types": "./dist/data-layer.d.ts",
      "import": "./dist/data-layer.js"
    },
    "./service-worker": {
      "types": "./dist/service-worker.d.ts",
      "import": "./dist/service-worker.js"
    },
    "./performance-budgets": {
      "types": "./dist/performance-budgets.d.ts",
      "import": "./dist/performance-budgets.js"
    },
    "./cost-tracking": {
      "types": "./dist/cost-tracking.d.ts",
      "import": "./dist/cost-tracking.js"
    },
    "./usage-analytics": {
      "types": "./dist/usage-analytics.d.ts",
      "import": "./dist/usage-analytics.js"
    },
    "./testing": {
      "types": "./dist/testing.d.ts",
      "import": "./dist/testing.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "lint": "echo 'Lint philjs-core'",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.16",
    "jsdom": "^27.0.0",
    "rollup": "^4.53.3",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
