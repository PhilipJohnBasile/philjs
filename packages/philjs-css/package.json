{
  "name": "@philjs/css",
  "version": "0.1.0",
  "type": "module",
  "description": "Type-safe CSS-in-TS solution for PhilJS with zero runtime overhead",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "css",
    "css-in-js",
    "css-in-ts",
    "type-safe",
    "atomic-css",
    "design-tokens",
    "theme",
    "variants",
    "zero-runtime"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "dependencies": {
    "csstype": "^3.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^24.0.0",
    "jest": "^29.5.0",
    "rollup": "^4.54.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": ">=4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/philjs.git",
    "directory": "packages/philjs-css"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme",
  "engines": {
    "node": ">=24"
  }
}
