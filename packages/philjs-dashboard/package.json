{
  "name": "@philjs/dashboard",
  "version": "0.1.0",
  "description": "Performance monitoring dashboard for PhilJS applications",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./collectors": {
      "types": "./dist/collectors/index.d.ts",
      "default": "./dist/collectors/index.js"
    }
  },
  "files": ["dist", "src"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "keywords": ["philjs", "performance", "monitoring", "dashboard", "metrics", "web-vitals"],
  "author": "PhilJS Team",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/sdk-trace-base": "^1.19.0",
    "@opentelemetry/sdk-trace-web": "^1.19.0",
    "idb": "^8.0.0",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@philjs/core": ">=0.1.0"
  },
  "engines": {
    "node": ">=24"
  }
}
