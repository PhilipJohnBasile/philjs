{
  "name": "@philjs/db",
  "version": "0.1.0",
  "description": "Database integration utilities for PhilJS - Prisma, Drizzle, Supabase, Migrations",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "philjs-db": "./dist/migrations/cli.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./supabase": "./dist/supabase.js",
    "./migrations": "./dist/migrations/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest"
  },
  "dependencies": {
    "@philjs/core": "workspace:*"
  },
  "peerDependencies": {
    "@prisma/client": "^7.2.0",
    "drizzle-orm": "^0.45.1",
    "@supabase/supabase-js": "^2.89.0"
  },
  "peerDependenciesMeta": {
    "@prisma/client": {
      "optional": true
    },
    "drizzle-orm": {
      "optional": true
    },
    "@supabase/supabase-js": {
      "optional": true
    }
  },
  "devDependencies": {
    "typescript": "6.0.0-dev.20251230",
    "vitest": "^4.0.16",
    "@types/node": "^25.0.0",
    "@prisma/client": "^7.2.0",
    "drizzle-orm": "^0.45.1",
    "@supabase/supabase-js": "^2.89.0"
  },
  "files": [
    "dist",
    "README.md",
    "MIGRATIONS.md"
  ],
  "keywords": [
    "philjs",
    "database",
    "prisma",
    "drizzle",
    "supabase",
    "orm",
    "migrations",
    "schema",
    "postgres",
    "mysql",
    "sqlite",
    "mongodb"
  ],
  "license": "MIT",
  "sideEffects": false,
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/philjs.git",
    "directory": "packages/philjs-db"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme",
  "engines": {
    "node": ">=24"
  }
}

