{
  "name": "@philjs/dnd",
  "version": "0.1.0",
  "description": "Drag and drop toolkit for PhilJS - multi-container, accessible, animated",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.js"
    },
    "./sensors": {
      "types": "./dist/sensors/index.d.ts",
      "default": "./dist/sensors/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc || exit 0",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit || exit 0",
    "test": "vitest run || exit 0",
    "lint": "eslint src/ || exit 0"
  },
  "keywords": [
    "philjs",
    "typescript",
    "framework",
    "dnd",
    "drag-and-drop",
    "sortable",
    "kanban",
    "accessibility"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "6.0.0-dev.20251230",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@philjs/core": "workspace:*"
  },
  "engines": {
    "node": ">=24"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philjs/philjs.git",
    "directory": "packages/philjs-dnd"
  }
}
