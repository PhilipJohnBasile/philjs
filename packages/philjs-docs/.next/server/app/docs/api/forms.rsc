0:["cd22Ei08xvul0IDkb5kRy",[[["",{"children":["docs",{"children":["api",{"children":["forms",{"children":["__PAGE__",{}]}]}]}]},"$undefined","$undefined",true],["",{"children":["docs",{"children":["api",{"children":["forms",{"children":["__PAGE__",{},[["$L1",["$","$L2",null,{"title":"philjs-forms","description":"Form handling primitives with validation, error handling, and server actions support.","sourceLink":"https://github.com/philjs/philjs/tree/main/packages/philjs-forms","methods":[{"name":"createForm","signature":"function createForm<T>(options: FormOptions<T>): FormReturn<T>","description":"Creates a form controller with validation, submission handling, and field management.","parameters":[{"name":"options","type":"FormOptions<T>","description":"Configuration object for the form"}],"returns":{"type":"FormReturn<T>","description":"Form controller with fields, handlers, and state"},"example":"const form = createForm({\n  initialValues: {\n    email: '',\n    password: '',\n  },\n  validate: (values) => {\n    const errors: Record<string, string> = {};\n    if (!values.email) errors.email = 'Required';\n    if (!values.email.includes('@')) errors.email = 'Invalid email';\n    if (values.password.length < 8) errors.password = 'Too short';\n    return errors;\n  },\n  onSubmit: async (values) => {\n    await signIn(values);\n  },\n});\n\n// In your component\n<form onSubmit={form.handleSubmit}>\n  <input {...form.field('email')} />\n  {form.errors.email && <span>{form.errors.email}</span>}\n\n  <input type=\"password\" {...form.field('password')} />\n  {form.errors.password && <span>{form.errors.password}</span>}\n\n  <button type=\"submit\" disabled={form.submitting}>\n    Submit\n  </button>\n</form>","since":"1.0.0"},{"name":"useForm","signature":"function useForm<T>(options: FormOptions<T>): FormReturn<T>","description":"Hook version of createForm for use within components.","parameters":[{"name":"options","type":"FormOptions<T>","description":"Configuration object for the form"}],"returns":{"type":"FormReturn<T>","description":"Form controller with fields, handlers, and state"},"example":"function LoginForm() {\n  const form = useForm({\n    initialValues: { email: '', password: '' },\n    onSubmit: async (values) => {\n      await login(values);\n    },\n  });\n\n  return (\n    <form onSubmit={form.handleSubmit}>\n      <input {...form.field('email')} />\n      <input type=\"password\" {...form.field('password')} />\n      <button type=\"submit\">Login</button>\n    </form>\n  );\n}","since":"1.0.0"},{"name":"createFormField","signature":"function createFormField<T>(name: string, options?: FieldOptions<T>): FormField<T>","description":"Creates an individual form field with its own validation and state.","parameters":[{"name":"name","type":"string","description":"Field name for form data"},{"name":"options","type":"FieldOptions<T>","description":"Field configuration","optional":true}],"returns":{"type":"FormField<T>","description":"Field controller with value, error, and handlers"},"example":"const emailField = createFormField('email', {\n  validate: (value) => {\n    if (!value) return 'Email is required';\n    if (!value.includes('@')) return 'Invalid email';\n  },\n  transform: (value) => value.toLowerCase().trim(),\n});\n\n<input\n  value={emailField.value()}\n  onInput={(e) => emailField.setValue(e.target.value)}\n  onBlur={emailField.handleBlur}\n/>\n{emailField.touched() && emailField.error() && (\n  <span>{emailField.error()}</span>\n)}","since":"1.0.0"},{"name":"useServerAction","signature":"function useServerAction<T, R>(action: (data: T) => Promise<R>): ServerActionReturn<T, R>","description":"Wraps a server action with pending state and error handling.","parameters":[{"name":"action","type":"(data: T) => Promise<R>","description":"Server action function"}],"returns":{"type":"ServerActionReturn<T, R>","description":"Action wrapper with execute, pending, and error state"},"example":"// server/actions.ts\n'use server';\nexport async function createUser(data: UserData) {\n  return db.users.create({ data });\n}\n\n// Component\nimport { createUser } from './server/actions';\n\nfunction CreateUserForm() {\n  const { execute, pending, error, data } = useServerAction(createUser);\n\n  return (\n    <form action={execute}>\n      <input name=\"name\" />\n      <input name=\"email\" type=\"email\" />\n      {pending() && <Spinner />}\n      {error() && <Alert>{error().message}</Alert>}\n      <button type=\"submit\">Create</button>\n    </form>\n  );\n}","since":"1.0.0"},{"name":"createValidator","signature":"function createValidator<T>(schema: ValidationSchema<T>): Validator<T>","description":"Creates a reusable validator from a schema definition.","parameters":[{"name":"schema","type":"ValidationSchema<T>","description":"Validation schema object"}],"returns":{"type":"Validator<T>","description":"Validator function"},"example":"const userValidator = createValidator({\n  email: {\n    required: 'Email is required',\n    pattern: [/^[^@]+@[^@]+$/, 'Invalid email format'],\n  },\n  password: {\n    required: 'Password is required',\n    minLength: [8, 'Password must be at least 8 characters'],\n    custom: (value) => {\n      if (!/[A-Z]/.test(value)) {\n        return 'Must contain uppercase letter';\n      }\n    },\n  },\n  age: {\n    min: [18, 'Must be 18 or older'],\n    max: [120, 'Invalid age'],\n  },\n});\n\nconst form = createForm({\n  validate: userValidator,\n  // ...\n});","since":"1.0.0"},{"name":"FormProvider","signature":"function FormProvider<T>(props: { form: FormReturn<T>; children: JSX.Element }): JSX.Element","description":"Provides form context to child components for nested form fields.","parameters":[{"name":"form","type":"FormReturn<T>","description":"Form controller instance"},{"name":"children","type":"JSX.Element","description":"Child components"}],"example":"function CheckoutForm() {\n  const form = createForm({ /* ... */ });\n\n  return (\n    <FormProvider form={form}>\n      <form onSubmit={form.handleSubmit}>\n        <ShippingFields />\n        <PaymentFields />\n        <button type=\"submit\">Complete Order</button>\n      </form>\n    </FormProvider>\n  );\n}\n\nfunction ShippingFields() {\n  const form = useFormContext();\n  return (\n    <>\n      <input {...form.field('address')} />\n      <input {...form.field('city')} />\n    </>\n  );\n}","since":"1.0.0"}],"types":[{"name":"FormOptions","kind":"interface","description":"Configuration options for createForm and useForm.","properties":[{"name":"initialValues","type":"T","description":"Initial form values"},{"name":"validate","type":"(values: T) => Record<string, string>","description":"Validation function","optional":true},{"name":"onSubmit","type":"(values: T) => void | Promise<void>","description":"Submit handler"},{"name":"validateOnChange","type":"boolean","description":"Validate on every change","optional":true,"default":"false"},{"name":"validateOnBlur","type":"boolean","description":"Validate on field blur","optional":true,"default":"true"}]},{"name":"FormReturn","kind":"interface","description":"Return value of createForm with all form controls.","properties":[{"name":"values","type":"Accessor<T>","description":"Current form values"},{"name":"errors","type":"Record<keyof T, string>","description":"Current validation errors"},{"name":"touched","type":"Record<keyof T, boolean>","description":"Fields that have been touched"},{"name":"submitting","type":"Accessor<boolean>","description":"Whether form is submitting"},{"name":"field","type":"(name: keyof T) => FieldProps","description":"Get props for a field"},{"name":"handleSubmit","type":"(e: Event) => void","description":"Form submit handler"},{"name":"reset","type":"() => void","description":"Reset form to initial values"},{"name":"setFieldValue","type":"(name: keyof T, value: any) => void","description":"Set a field value programmatically"}]},{"name":"ValidationSchema","kind":"type","descripti