{
  "name": "philjs-docs",
  "version": "0.0.1",
  "private": true,
  "description": "Documentation site and interactive playground for PhilJS",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:static": "cross-env STATIC_EXPORT=true next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "deploy:vercel": "vercel deploy --prod",
    "deploy:vercel:preview": "vercel deploy",
    "deploy:cloudflare": "pnpm build:static && wrangler pages deploy out --project-name=philjs-docs",
    "deploy:netlify": "netlify deploy --prod --dir=.next",
    "deploy:netlify:preview": "netlify deploy --dir=.next"
  },
  "dependencies": {
    "@codemirror/lang-css": "^6.2.0",
    "@codemirror/lang-html": "^6.4.0",
    "@codemirror/lang-javascript": "^6.2.0",
    "@codemirror/lang-rust": "^6.0.1",
    "@codemirror/theme-one-dark": "^6.1.0",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "^14.2.0",
    "@uiw/react-codemirror": "^4.21.0",
    "clsx": "^2.1.0",
    "codemirror": "^6.0.0",
    "framer-motion": "^11.0.0",
    "fuse.js": "^7.0.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.300.0",
    "next": "^14.2.0",
    "next-mdx-remote": "^4.4.1",
    "philjs-core": "workspace:*",
    "philjs-forms": "workspace:*",
    "philjs-playground": "workspace:*",
    "philjs-router": "workspace:*",
    "philjs-ui": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.13.0",
    "rehype-slug": "^6.0.0",
    "shiki": "^1.1.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/mdx": "^2.0.10",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.0"
  },
  "author": "PhilJS Team",
  "license": "MIT"
}
