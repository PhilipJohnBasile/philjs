    "@tiptap/starter-kit": "^2.1.0",
    "dompurify": "^3.0.6",
    "highlight.js": "^11.9.0",
    "katex": "^0.16.9",
    "lowlight": "^3.1.0",
    "marked": "^11.1.0",
    "prosemirror-commands": "^1.5.2",
    "prosemirror-history": "^1.3.2",
    "prosemirror-keymap": "^1.2.2",
