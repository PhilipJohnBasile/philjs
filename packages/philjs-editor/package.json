    "prosemirror-state": "^1.4.3",
    "prosemirror-transform": "^1.8.0",
    "prosemirror-view": "^1.32.7",
    "tippy.js": "^6.3.7",
    "turndown": "^7.1.2",
    "y-prosemirror": "^1.2.5",
    "y-webrtc": "^10.2.5",
    "y-websocket": "^1.5.1",
    "yjs": "^13.6.10"
