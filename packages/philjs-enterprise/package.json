{
  "name": "@philjs/enterprise",
  "version": "0.1.0",
  "description": "Enterprise features for PhilJS - Multi-tenancy, SSO, RBAC, Audit Logging, Feature Flags",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./multi-tenancy": {
      "types": "./dist/multi-tenancy.d.ts",
      "import": "./dist/multi-tenancy.js"
    },
    "./sso": {
      "types": "./dist/sso.d.ts",
      "import": "./dist/sso.js"
    },
    "./rbac": {
      "types": "./dist/rbac.d.ts",
      "import": "./dist/rbac.js"
    },
    "./audit": {
      "types": "./dist/audit.d.ts",
      "import": "./dist/audit.js"
    },
    "./feature-flags": {
      "types": "./dist/feature-flags.d.ts",
      "import": "./dist/feature-flags.js"
    },
    "./white-label": {
      "types": "./dist/white-label.d.ts",
      "import": "./dist/white-label.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/multi-tenancy.ts src/sso.ts src/rbac.ts src/audit.ts src/feature-flags.ts src/white-label.ts --format esm --dts",
    "dev": "tsup src/*.ts --format esm --dts --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "tsup": "^8.5.1",
    "typescript": "6.0.0-dev.20251230",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "philjs",
    "enterprise",
    "multi-tenancy",
    "sso",
    "saml",
    "oidc",
    "ldap",
    "rbac",
    "audit",
    "feature-flags",
    "white-label"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=24"
  }
}

