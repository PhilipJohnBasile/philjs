{
  "name": "@philjs/export",
  "version": "0.1.0",
  "description": "Data export utilities for PhilJS - CSV, Excel, JSON, XML, PDF, and more",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./formats": {
      "types": "./dist/formats/index.d.ts",
      "import": "./dist/formats/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "import": "./dist/hooks.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "xlsx": "^0.18.5",
    "papaparse": "^5.4.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/papaparse": "^5.3.14",
    "react": "^18.2.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "keywords": [
    "philjs",
    "export",
    "csv",
    "excel",
    "json",
    "xml",
    "pdf",
    "download",
    "data-export"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/philjs/philjs.git",
    "directory": "packages/philjs-export"
  }
}
