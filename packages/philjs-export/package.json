{
  "name": "@philjs/export",
  "version": "0.1.0",
  "description": "Data export utilities for PhilJS - CSV, Excel, JSON, PDF",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./formats": {
      "types": "./dist/formats/index.d.ts",
      "default": "./dist/formats/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc || exit 0",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit || exit 0",
    "test": "vitest run || exit 0",
    "lint": "eslint src/ || exit 0"
  },
  "keywords": [
    "philjs",
    "typescript",
    "framework",
    "export",
    "csv",
    "excel",
    "json",
    "pdf",
    "download"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "dependencies": {
    "xlsx": "^0.18.5",
    "papaparse": "^5.4.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/papaparse": "^5.3.14",
    "tsup": "^8.5.1",
    "typescript": "6.0.0-dev.20251230",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@philjs/core": "workspace:*"
  },
  "engines": {
    "node": ">=24"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philjs/philjs.git",
    "directory": "packages/philjs-export"
  }
}
