{
  "name": "philjs-graphql",
  "version": "2.0.0",
  "description": "GraphQL integration for PhilJS with typed queries and mutations",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "lint": "echo 'Lint philjs-graphql'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "philjs-core": "workspace:*",
    "philjs-ssr": "workspace:*"
  },
  "devDependencies": {
    "graphql": "^16.9.0",
    "@graphql-tools/schema": "^10.0.4",
    "@graphql-tools/utils": "^10.5.7",
    "vitest": "^3.2.4",
    "typescript": "^5.7.2",
    "rollup": "^4.54.0"
  },
  "peerDependencies": {
    "graphql": "^16.0.0"
  },
  "sideEffects": false,
  "license": "MIT",
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/philjs.git",
    "directory": "packages/philjs-graphql"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme"
}
