{
  "name": "@philjs/islands",
  "version": "0.1.0",
  "description": "Islands architecture with selective hydration for PhilJS - pure web components",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "src"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "keywords": [
    "philjs",
    "islands",
    "islands-architecture",
    "selective-hydration",
    "web-components",
    "partial-hydration",
    "progressive-enhancement",
    "lazy-loading"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@philjs/core": ">=0.1.0"
  },
  "engines": {
    "node": ">=18"
  }
}
