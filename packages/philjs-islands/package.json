{
  "name": "philjs-islands",
  "version": "2.0.0",
  "description": "Islands architecture with multi-framework support and selective hydration for PhilJS",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.js"
    },
    "./multi-framework": {
      "types": "./dist/multi-framework.d.ts",
      "import": "./dist/multi-framework.js"
    },
    "./framework-bridge": {
      "types": "./dist/framework-bridge.d.ts",
      "import": "./dist/framework-bridge.js"
    },
    "./vite-multi-framework": {
      "types": "./dist/vite-multi-framework.d.ts",
      "import": "./dist/vite-multi-framework.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "echo 'Lint philjs-islands'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "vue": "^3.0.0",
    "svelte": "^4.0.0 || ^5.0.0",
    "preact": "^10.0.0",
    "solid-js": "^1.0.0",
    "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "preact": {
      "optional": true
    },
    "solid-js": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "rollup": "^4.28.1",
    "solid-js": "^1.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  },
  "sideEffects": false,
  "license": "MIT",
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/philjs.git",
    "directory": "packages/philjs-islands"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme",
  "keywords": [
    "philjs",
    "islands-architecture",
    "hydration",
    "multi-framework",
    "react",
    "vue",
    "svelte",
    "preact",
    "solid",
    "astro-style",
    "partial-hydration",
    "progressive-enhancement"
  ]
}
