{
  "name": "philjs-maps",
  "version": "0.0.1",
  "description": "Map components for PhilJS with support for Google Maps, Mapbox, and Leaflet",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./providers/google": {
      "types": "./dist/providers/google.d.ts",
      "import": "./dist/providers/google.js"
    },
    "./providers/mapbox": {
      "types": "./dist/providers/mapbox.d.ts",
      "import": "./dist/providers/mapbox.js"
    },
    "./providers/leaflet": {
      "types": "./dist/providers/leaflet.d.ts",
      "import": "./dist/providers/leaflet.js"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "import": "./dist/hooks.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "lint": "echo 'Lint philjs-maps'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "mapbox-gl": "^3.3.0",
    "@react-google-maps/api": "^2.19.3",
    "supercluster": "^8.0.1"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.8",
    "@types/supercluster": "^7.1.3",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^27.0.0",
    "rollup": "^4.53.3",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "philjs-core": "workspace:*"
  },
  "license": "MIT",
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/philjs.git",
    "directory": "packages/philjs-maps"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme",
  "keywords": [
    "philjs",
    "maps",
    "google-maps",
    "mapbox",
    "leaflet",
    "geolocation",
    "gis"
  ]
}
