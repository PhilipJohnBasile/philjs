{
  "name": "@philjs/maps",
  "version": "0.1.0",
  "description": "Map components for PhilJS with support for Google Maps, Mapbox, and Leaflet",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./google": {
      "types": "./dist/providers/google.d.ts",
      "default": "./dist/providers/google.js"
    },
    "./mapbox": {
      "types": "./dist/providers/mapbox.d.ts",
      "default": "./dist/providers/mapbox.js"
    },
    "./leaflet": {
      "types": "./dist/providers/leaflet.d.ts",
      "default": "./dist/providers/leaflet.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    }
  },
  "files": ["dist", "src"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "keywords": [
    "philjs",
    "maps",
    "google-maps",
    "mapbox",
    "leaflet",
    "geolocation",
    "gis",
    "web-components"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "dependencies": {
    "leaflet": "^1.9.4",
    "mapbox-gl": "^3.3.0",
    "supercluster": "^8.0.1"
  },
  "devDependencies": {
    "@types/google.maps": "^3.55.0",
    "@types/leaflet": "^1.9.8",
    "@types/supercluster": "^7.1.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@philjs/core": ">=0.1.0"
  },
  "engines": {
    "node": ">=24"
  }
}
