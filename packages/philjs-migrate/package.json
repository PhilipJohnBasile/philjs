{
  "name": "@philjs/migrate",
  "version": "0.1.0",
  "description": "Migration CLI for PhilJS - migrate from React, Vue, Angular, Svelte",
  "type": "module",
  "bin": {
    "philjs-migrate": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format esm --dts"
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/generator": "^7.28.5",
    "commander": "^14.0.2",
    "glob": "^13.0.0",
    "ora": "^8.0.0",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "tsup": "^8.5.1",
    "typescript": "6.0.0-dev.20251230"
  },
  "license": "MIT",
  "engines": {
    "node": ">=24"
  }
}

