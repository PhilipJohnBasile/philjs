{
  "name": "philjs-native",
  "version": "2.0.0",
  "description": "Cross-platform mobile development for PhilJS - React Native/Capacitor alternative",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./navigation": {
      "types": "./dist/navigation.d.ts",
      "import": "./dist/navigation.js"
    },
    "./apis": {
      "types": "./dist/apis/index.d.ts",
      "import": "./dist/apis/index.js"
    },
    "./styles": {
      "types": "./dist/styles.d.ts",
      "import": "./dist/styles.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    }
  },
  "bin": {
    "philjs-native": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "echo 'Lint philjs-native'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "philjs-core": "workspace:*"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^27.0.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "philjs-core": ">=2.0.0"
  },
  "license": "MIT",
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/philjs.git",
    "directory": "packages/philjs-native"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme",
  "keywords": [
    "philjs",
    "native",
    "mobile",
    "ios",
    "android",
    "cross-platform",
    "react-native-alternative",
    "capacitor-alternative"
  ]
}
