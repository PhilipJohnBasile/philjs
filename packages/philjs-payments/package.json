{
  "name": "@philjs/payments",
  "version": "0.1.0",
  "description": "Payment processing for PhilJS - Stripe, PayPal, Square support",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./providers/stripe": {
      "types": "./dist/providers/stripe.d.ts",
      "default": "./dist/providers/stripe.js"
    },
    "./providers/paypal": {
      "types": "./dist/providers/paypal.d.ts",
      "default": "./dist/providers/paypal.js"
    },
    "./providers/square": {
      "types": "./dist/providers/square.d.ts",
      "default": "./dist/providers/square.js"
    },
    "./webhooks": {
      "types": "./dist/webhooks/index.d.ts",
      "default": "./dist/webhooks/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "keywords": [
    "philjs",
    "payments",
    "stripe",
    "paypal",
    "square",
    "subscriptions"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "dependencies": {
    "stripe": "^20.1.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "square": "^35.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "tsup": "^8.5.1",
    "typescript": "^6.0.0",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@philjs/core": "workspace:*"
  },
  "engines": {
    "node": ">=24"
  }
}
