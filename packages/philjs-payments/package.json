{
  "name": "@philjs/payments",
  "version": "0.1.0",
  "description": "Payment processing for PhilJS - Stripe, PayPal, Square support",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./providers/stripe": {
      "types": "./dist/providers/stripe.d.ts",
      "import": "./dist/providers/stripe.js",
      "require": "./dist/providers/stripe.cjs"
    },
    "./providers/paypal": {
      "types": "./dist/providers/paypal.d.ts",
      "import": "./dist/providers/paypal.js",
      "require": "./dist/providers/paypal.cjs"
    },
    "./providers/square": {
      "types": "./dist/providers/square.d.ts",
      "import": "./dist/providers/square.js",
      "require": "./dist/providers/square.cjs"
    },
    "./webhooks": {
      "types": "./dist/webhooks/index.d.ts",
      "import": "./dist/webhooks/index.js",
      "require": "./dist/webhooks/index.cjs"
    }
  },
  "files": ["dist", "src"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "keywords": ["philjs", "payments", "stripe", "paypal", "square", "subscriptions"],
  "author": "PhilJS Team",
  "license": "MIT",
  "dependencies": {
    "stripe": "^14.0.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "square": "^35.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@philjs/core": ">=0.1.0"
  },
  "engines": {
    "node": ">=18"
  }
}
