{
  "name": "@philjs/perf",
  "version": "0.1.0",
  "description": "High-performance runtime utilities for PhilJS - memoization, batching, pooling, lazy evaluation",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./memo": {
      "types": "./dist/memo.d.ts",
      "default": "./dist/memo.js"
    },
    "./batch": {
      "types": "./dist/batch.d.ts",
      "default": "./dist/batch.js"
    },
    "./pool": {
      "types": "./dist/pool.d.ts",
      "default": "./dist/pool.js"
    },
    "./lazy": {
      "types": "./dist/lazy.d.ts",
      "default": "./dist/lazy.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc || exit 0",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit || exit 0",
    "test": "vitest run || exit 0",
    "bench": "vitest bench"
  },
  "keywords": [
    "philjs",
    "typescript",
    "framework",
    "perf",
    "performance",
    "memoization",
    "batching",
    "object-pooling",
    "lazy-evaluation",
    "optimization"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "6.0.0-dev.20251230",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=24"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philjs/philjs.git",
    "directory": "packages/philjs-perf"
  }
}
