{
  "name": "@philjs/plugin-analytics",
  "version": "0.1.0",
  "description": "Analytics integration plugin for PhilJS (GA4, Plausible, Mixpanel, and more)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "echo 'Lint philjs-plugin-analytics'",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "philjs",
    "plugin",
    "analytics",
    "tracking",
    "ga4",
    "google-analytics",
    "plausible",
    "mixpanel",
    "amplitude",
    "segment",
    "posthog",
    "umami",
    "fathom",
    "privacy",
    "gdpr"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "dependencies": {
    "@philjs/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^27.4.0",
    "rollup": "^4.54.0",
    "typescript": "6.0.0-dev.20251230",
    "vitest": "^4.0.16"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/philjs.git",
    "directory": "packages/philjs-plugin-analytics"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme",
  "engines": {
    "node": ">=24"
  }
}

