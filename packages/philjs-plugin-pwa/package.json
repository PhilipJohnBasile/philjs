{
  "name": "@philjs/plugin-pwa",
  "version": "0.1.0",
  "description": "Progressive Web App (PWA) plugin for PhilJS with service workers and offline support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc || exit 0",
    "test": "vitest run || exit 0",
    "lint": "npm run typecheck || exit 0",
    "typecheck": "tsc --noEmit || exit 0"
  },
  "dependencies": {
    "@philjs/core": "workspace:*"
  },
  "devDependencies": {
    "rollup": "^4.54.0",
    "typescript": "6.0.0-dev.20251230",
    "vitest": "^4.0.16"
  },
  "sideEffects": false,
  "license": "MIT",
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/philjs/philjs.git",
    "directory": "packages/philjs-plugin-pwa"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme",
  "keywords": [
    "philjs",
    "typescript",
    "framework",
    "plugin",
    "pwa",
    "progressive-web-app",
    "service-worker",
    "offline"
