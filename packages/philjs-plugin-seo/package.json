{
  "name": "@philjs/plugin-seo",
  "version": "0.1.0",
  "description": "SEO plugin for PhilJS with meta tags, OpenGraph, JSON-LD, and sitemap generation",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./head": {
      "types": "./dist/head.d.ts",
      "import": "./dist/head.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc || exit 0",
    "dev": "tsc --watch",
    "test": "vitest run || exit 0",
    "test:watch": "vitest",
    "lint": "npm run typecheck || exit 0",
    "typecheck": "tsc --noEmit || exit 0"
  },
  "keywords": [
    "philjs",
    "typescript",
    "framework",
    "plugin",
    "seo",
    "meta-tags",
    "opengraph",
    "twitter-cards",
    "json-ld",
    "structured-data",
    "sitemap",
    "robots"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "dependencies": {
    "@philjs/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "typescript": "6.0.0-dev.20251230",
    "vitest": "^4.0.16"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/philjs/philjs.git",
    "directory": "packages/philjs-plugin-seo"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme",
  "engines": {
    "node": ">=24"
  }
}
