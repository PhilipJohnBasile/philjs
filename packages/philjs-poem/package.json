{
  "name": "@philjs/poem",
  "version": "0.1.0",
  "type": "module",
  "description": "Poem web framework integration for PhilJS - SSR, middleware, extractors, and WebSocket support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "default": "./dist/middleware.js"
    },
    "./extractors": {
      "types": "./dist/extractors.d.ts",
      "default": "./dist/extractors.js"
    },
    "./endpoints": {
      "types": "./dist/endpoints.d.ts",
      "default": "./dist/endpoints.js"
    },
    "./responses": {
      "types": "./dist/responses.d.ts",
      "default": "./dist/responses.js"
    },
    "./websocket": {
      "types": "./dist/websocket.d.ts",
      "default": "./dist/websocket.js"
    },
    "./openapi": {
      "types": "./dist/openapi.d.ts",
      "default": "./dist/openapi.js"
    },
    "./ssr": {
      "types": "./dist/ssr.d.ts",
      "default": "./dist/ssr.js"
    }
  },
  "files": [
    "dist",
    "src",
    "rust"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "build:rust": "cd rust && cargo build --release"
  },
  "dependencies": {
    "@philjs/core": "workspace:*",
    "@philjs/ssr": "workspace:*",
    "@philjs/liveview": "workspace:*"
  },
  "devDependencies": {
    "typescript": "6.0.0-dev.20251230",
    "tsup": "^8.5.1",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@philjs/core": "workspace:*"
  },
  "keywords": [
    "philjs",
    "poem",
    "rust",
    "ssr",
    "websocket",
    "middleware",
    "extractor",
    "openapi",
    "swagger"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anthropics/philjs",
    "directory": "packages/philjs-poem"
  },
  "engines": {
    "node": ">=24"
  }
}

