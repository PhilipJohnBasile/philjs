{
  "name": "@philjs/pwa",
  "version": "0.1.0",
  "type": "module",
  "description": "Zero-config PWA generation for PhilJS - service worker, manifest, push notifications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "lint": "eslint src/",
    "test": "vitest"
  },
  "keywords": [
    "philjs",
    "pwa",
    "service-worker",
    "manifest",
    "push-notifications",
    "offline"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "peerDependencies": {
    "@philjs/core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^6.0.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=24"
  }
}
