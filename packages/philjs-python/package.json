{
  "name": "@philjs/python",
  "version": "0.1.0",
  "description": "Python bindings for PhilJS AI/ML server functions - LLM, embeddings, and ML model serving",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "philjs-python": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    },
    "./llm": {
      "types": "./dist/llm.d.ts",
      "import": "./dist/llm.js"
    },
    "./embeddings": {
      "types": "./dist/embeddings.d.ts",
      "import": "./dist/embeddings.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    }
  },
  "files": [
    "dist",
    "python",
    "templates"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "py:test": "cd python && pytest",
    "py:lint": "cd python && ruff check ."
  },
  "dependencies": {
    "execa": "^9.5.2"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "tsup": "^8.5.1",
    "typescript": "next",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@philjs/core": "workspace:*",
    "@philjs/ai": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@philjs/core": {
      "optional": false
    },
    "@philjs/ai": {
      "optional": true
    }
  },
  "keywords": [
    "philjs",
    "python",
    "ai",
    "ml",
    "llm",
    "embeddings",
    "openai",
    "anthropic",
    "langchain",
    "transformers",
    "pytorch",
    "tensorflow"
  ],
  "license": "MIT",
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/PhilipJohnBasile/philjs.git",
    "directory": "packages/philjs-python"
  }
}
