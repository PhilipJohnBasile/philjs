{
  "name": "philjs-react-compat",
  "version": "2.0.0",
  "description": "React compatibility layer for seamless migration to PhilJS",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./codemod": {
      "types": "./dist/codemod/transform.d.ts",
      "import": "./dist/codemod/transform.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "lint": "echo 'Lint philjs-react-compat'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "philjs-core": "workspace:*"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@types/node": "^22.5.5",
    "@vitest/coverage-v8": "^3.2.4",
    "jscodeshift": "^0.15.0",
    "rollup": "^4.53.5",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "philjs",
    "react",
    "compatibility",
    "hooks",
    "migration",
    "compat"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/philjs.git",
    "directory": "packages/philjs-react-compat"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme"
}
