{
  "name": "philjs-realtime",
  "version": "0.0.1",
  "description": "Real-time collaboration, WebSockets, presence, and multiplayer state for PhilJS",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./presence": {
      "types": "./dist/presence.d.ts",
      "import": "./dist/presence.js"
    },
    "./yjs": {
      "types": "./dist/yjs.d.ts",
      "import": "./dist/yjs.js"
    },
    "./cursors": {
      "types": "./dist/cursors.d.ts",
      "import": "./dist/cursors.js"
    },
    "./rooms": {
      "types": "./dist/rooms.d.ts",
      "import": "./dist/rooms.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "philjs-core": "workspace:*"
  },
  "peerDependencies": {
    "yjs": "^13.0.0",
    "ws": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "yjs": { "optional": true },
    "ws": { "optional": true }
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "vitest": "^3.1.1"
  },
  "keywords": ["philjs", "realtime", "websocket", "collaboration", "presence", "multiplayer", "crdt"],
  "license": "MIT"
}
