{
  "name": "@philjs/resumable",
  "version": "0.1.0",
  "description": "Qwik-style resumability for PhilJS - zero hydration overhead through state serialization and lazy resumption",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./serializer": {
      "types": "./dist/serializer.d.ts",
      "default": "./dist/serializer.js"
    },
    "./deserializer": {
      "types": "./dist/deserializer.d.ts",
      "default": "./dist/deserializer.js"
    },
    "./qrl": {
      "types": "./dist/qrl.d.ts",
      "default": "./dist/qrl.js"
    },
    "./triggers": {
      "types": "./dist/triggers.d.ts",
      "default": "./dist/triggers.js"
    },
    "./loader": {
      "types": "./dist/loader.d.ts",
      "default": "./dist/loader.js"
    },
    "./hydration": {
      "types": "./dist/hydration.d.ts",
      "default": "./dist/hydration.js"
    },
    "./resumable": {
      "types": "./dist/resumable.d.ts",
      "default": "./dist/resumable.js"
    },
    "./container": {
      "types": "./dist/container.d.ts",
      "default": "./dist/container.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc || exit 0",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit || exit 0",
    "test": "vitest run || exit 0",
    "lint": "eslint src/ || exit 0"
  },
  "keywords": [
    "philjs",
    "typescript",
    "framework",
    "resumable",
    "resumability",
    "partial-hydration",
    "zero-javascript",
    "lazy-loading",
    "islands-architecture",
    "progressive-hydration",
    "server-side-rendering"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "6.0.0-dev.20251230",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@philjs/core": "workspace:*",
    "vite": "^7.3.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=24"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philjs/philjs.git",
    "directory": "packages/philjs-resumable"
  }
}
