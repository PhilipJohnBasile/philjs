{
  "name": "@philjs/resumable",
  "version": "0.1.0",
  "description": "Resumability and partial hydration system for PhilJS - zero-JS-until-interaction",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./serializer": {
      "types": "./dist/serializer.d.ts",
      "default": "./dist/serializer.js"
    },
    "./loader": {
      "types": "./dist/loader.d.ts",
      "default": "./dist/loader.js"
    },
    "./hydration": {
      "types": "./dist/hydration.d.ts",
      "default": "./dist/hydration.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "keywords": [
    "philjs",
    "resumability",
    "partial-hydration",
    "zero-javascript",
    "lazy-loading",
    "islands-architecture",
    "progressive-hydration",
    "server-side-rendering"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^6.0.0",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@philjs/core": "workspace:*",
    "vite": "^7.3.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=24"
  }
}
