{
  "name": "@philjs/rocket",
  "version": "0.1.0",
  "type": "module",
  "description": "Rocket web framework integration for PhilJS - SSR, fairings, guards, and WebSocket support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./app": {
      "types": "./dist/app.d.ts",
      "default": "./dist/app.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "default": "./dist/middleware.js"
    },
    "./fairing": {
      "types": "./dist/fairing.d.ts",
      "default": "./dist/fairing.js"
    },
    "./guards": {
      "types": "./dist/guards.d.ts",
      "default": "./dist/guards.js"
    },
    "./handlers": {
      "types": "./dist/handlers.d.ts",
      "default": "./dist/handlers.js"
    },
    "./responders": {
      "types": "./dist/responders.d.ts",
      "default": "./dist/responders.js"
    },
    "./ssr": {
      "types": "./dist/ssr.d.ts",
      "default": "./dist/ssr.js"
    },
    "./state": {
      "types": "./dist/state.d.ts",
      "default": "./dist/state.js"
    },
    "./templates": {
      "types": "./dist/templates.d.ts",
      "default": "./dist/templates.js"
    },
    "./websocket": {
      "types": "./dist/websocket.d.ts",
      "default": "./dist/websocket.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    },
    "./forms": {
      "types": "./dist/forms.d.ts",
      "default": "./dist/forms.js"
    },
    "./cookies": {
      "types": "./dist/cookies.d.ts",
      "default": "./dist/cookies.js"
    }
  },
  "files": [
    "dist",
    "src",
    "rust"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsup src/index.ts src/server.ts src/handlers.ts src/middleware.ts src/fairing.ts src/guards.ts src/responders.ts src/templates.ts src/websocket.ts src/state.ts src/forms.ts src/cookies.ts src/ssr.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "build:rust": "cargo build --release"
  },
  "dependencies": {
    "@philjs/core": "workspace:*",
    "@philjs/ssr": "workspace:*",
    "@philjs/liveview": "workspace:*"
  },
  "devDependencies": {
    "typescript": "6.0.0-dev.20251230",
    "tsup": "^8.5.1",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@philjs/core": "workspace:*"
  },
  "keywords": [
    "philjs",
    "rocket",
    "rust",
    "ssr",
    "websocket",
    "fairing",
    "guard"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anthropics/philjs",
    "directory": "packages/philjs-rocket"
  },
  "engines": {
    "node": ">=24"
  }
}

