{
  "name": "@philjs/router-typesafe",
  "version": "0.1.0",
  "description": "Type-safe router for PhilJS with fully typed params and Zod validation",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc || exit 0",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit || exit 0",
    "test": "vitest run || exit 0",
    "lint": "eslint src/ || exit 0"
  },
  "keywords": [
    "philjs",
    "typescript",
    "framework",
    "router",
    "typesafe",
    "routing",
    "navigation",
    "type-safe",
    "zod",
    "validation"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "6.0.0-dev.20251230",
    "vitest": "^4.0.16",
    "zod": "^4.2.1"
  },
  "peerDependencies": {
    "@philjs/core": "workspace:*",
    "typescript": "6.0.0-dev.20251230",
    "zod": "^4.2.1"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=24"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philjs/philjs.git",
    "directory": "packages/philjs-router-typesafe"
  }
}
