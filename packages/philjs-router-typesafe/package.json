{
  "name": "@philjs/router-typesafe",
  "version": "0.1.0",
  "description": "Type-safe router for PhilJS with fully typed params and Zod validation",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "src"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "keywords": [
    "philjs",
    "router",
    "routing",
    "navigation",
    "type-safe",
    "typescript",
    "zod",
    "validation"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "zod": "^4.2.1"
  },
  "peerDependencies": {
    "@philjs/core": ">=0.1.0",
    "zod": "^4.2.1"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=24"
  }
}
