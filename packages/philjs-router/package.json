{
  "name": "@philjs/router",
  "version": "0.1.0",
  "description": "File-based routing with nested layouts for PhilJS",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./file-based": {
      "types": "./dist/file-based/index.d.ts",
      "import": "./dist/file-based/index.js"
    },
    "./file-based/vite": {
      "types": "./dist/file-based/vite-plugin.d.ts",
      "import": "./dist/file-based/vite-plugin.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript || exit 0",
    "test": "vitest run || exit 0",
    "lint": "npm run typecheck || exit 0",
    "typecheck": "tsc --noEmit || exit 0"
  },
  "dependencies": {
    "@philjs/core": "workspace:*"
  },
  "devDependencies": {
    "rollup": "^4.55.1",
    "typescript": "6.0.0-dev.20251230",
    "vitest": "^4.0.16"
  },
  "sideEffects": false,
  "license": "MIT",
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/philjs/philjs.git",
    "directory": "packages/philjs-router"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme",
  "keywords": [
    "philjs",
    "typescript",
    "framework",
    "router",
    "routing",
    "navigation",
    "file-based-routing",
    "nextjs-style",
    "vite-plugin"
  ],
  "engines": {
    "node": ">=24"
  }
}
