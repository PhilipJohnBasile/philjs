{
  "name": "@philjs/rpc",
  "version": "0.1.0",
  "description": "tRPC-style end-to-end type-safe RPC system for PhilJS",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "lint": "echo 'Lint philjs-rpc'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@philjs/core": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "zod": "^4.2.1"
  },
  "peerDependencies": {
    "zod": "^4.2.1"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "license": "MIT",
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/philjs.git",
    "directory": "packages/philjs-rpc"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme",
  "keywords": [
    "philjs",
    "rpc",
    "trpc",
    "type-safe",
    "api",
    "typescript"
  ],
  "engines": {
    "node": ">=24"
  }
}
