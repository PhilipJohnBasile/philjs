{
  "name": "philjs-ssr",
  "version": "0.1.0",
  "description": "SSR streaming, loaders, actions, and resumability for PhilJS",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "lint": "echo 'Lint philjs-ssr'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "philjs-core": "workspace:*",
    "philjs-router": "workspace:*"
  },
  "devDependencies": {
    "rollup": "^4.28.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
