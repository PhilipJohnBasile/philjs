{
  "name": "@philjs/studio",
  "version": "0.1.0",
  "description": "Visual component builder with drag-and-drop for PhilJS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "src"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "keywords": ["philjs", "studio", "visual-builder", "drag-and-drop", "no-code"],
  "author": "PhilJS Team",
  "license": "MIT",
  "dependencies": {
    "immer": "^10.0.3",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@philjs/core": ">=0.1.0",
    "@philjs/ui": ">=0.1.0",
    "@philjs/dnd": ">=0.1.0"
  },
  "engines": {
    "node": ">=24"
  }
}
