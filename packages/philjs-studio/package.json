{
  "name": "@philjs/studio",
  "version": "0.1.0",
  "description": "Visual component builder with drag-and-drop for PhilJS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": ["dist", "src"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "keywords": ["philjs", "studio", "visual-builder", "drag-and-drop", "no-code"],
  "author": "PhilJS Team",
  "license": "MIT",
  "dependencies": {
    "immer": "^11.1.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@philjs/core": "workspace:*",
    "@philjs/ui": "workspace:*",
    "@philjs/dnd": "workspace:*"
  },
  "engines": {
    "node": ">=24"
  }
}
