{
  "name": "@philjs/swift",
  "version": "0.0.1",
  "description": "Swift bindings for PhilJS - iOS and macOS native app development",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./ios": {
      "types": "./dist/ios.d.ts",
      "import": "./dist/ios.js"
    },
    "./macos": {
      "types": "./dist/macos.d.ts",
      "import": "./dist/macos.js"
    }
  },
  "files": ["dist", "swift", "templates"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsup": "^8.0.0",
    "typescript": "next",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@philjs/core": "workspace:*"
  },
  "keywords": ["philjs", "swift", "ios", "macos", "native", "swiftui", "uikit", "appkit"],
  "license": "MIT",
  "author": "PhilJS Team"
}
