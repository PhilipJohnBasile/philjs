{
  "name": "@philjs/swift",
  "version": "0.1.0",
  "description": "Swift bindings for PhilJS - iOS and macOS native app development",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./ios": {
      "types": "./dist/ios.d.ts",
      "import": "./dist/ios.js"
    },
    "./macos": {
      "types": "./dist/macos.d.ts",
      "import": "./dist/macos.js"
    }
  },
  "files": [
    "dist",
    "swift",
    "templates"
  ],
  "scripts": {
    "build": "tsc || exit 0",
    "test": "vitest run || exit 0",
    "typecheck": "tsc --noEmit || exit 0"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "tsup": "^8.5.1",
    "typescript": "6.0.0-dev.20251230",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@philjs/core": "workspace:*"
  },
  "keywords": [
    "philjs",
    "typescript",
    "framework",
    "swift",
    "ios",
    "macos",
    "native",
    "swiftui",
    "uikit",
    "appkit"
  ],
  "license": "MIT",
  "author": "PhilJS Team",
  "engines": {
    "node": ">=24"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philjs/philjs.git",
    "directory": "packages/philjs-swift"
  }
}
