{
  "name": "@philjs/table",
  "version": "0.0.1",
  "description": "Headless, type-safe table component for PhilJS - TanStack Table inspired",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "test": "vitest run"
  },
  "peerDependencies": {
    "philjs-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "keywords": ["table", "datagrid", "headless", "sorting", "filtering", "pagination", "philjs"],
  "license": "MIT",
  "sideEffects": false
}
