{
  "name": "@philjs/virtual",
  "version": "0.1.0",
  "description": "High-performance list virtualization for PhilJS - render millions of items efficiently",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "test": "vitest run"
  },
  "peerDependencies": {
    "@philjs/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "tsup": "^8.5.1",
    "typescript": "^6.0.0",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "virtualization",
    "virtual-list",
    "virtual-scroll",
    "windowing",
    "philjs"
  ],
  "license": "MIT",
  "sideEffects": false,
  "engines": {
    "node": ">=24"
  }
}
