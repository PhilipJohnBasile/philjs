{
  "name": "@philjs/wasm",
  "version": "0.1.0",
  "description": "Rust/WebAssembly integration for PhilJS with reactive signals",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "philjs-wasm": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite-plugin.d.ts",
      "import": "./dist/vite-plugin.js"
    },
    "./codegen": {
      "types": "./dist/codegen.d.ts",
      "import": "./dist/codegen.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript || exit 0",
    "test": "vitest run || exit 0",
    "test:watch": "vitest",
    "lint": "npm run typecheck || exit 0",
    "typecheck": "tsc --noEmit || exit 0"
  },
  "dependencies": {},
  "peerDependencies": {
    "@philjs/core": "workspace:*",
    "vite": "^7.3.0"
  },
  "peerDependenciesMeta": {
    "@philjs/core": {
      "optional": false
    },
    "vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "@philjs/core": "workspace:*",
    "rollup": "^4.54.0",
    "typescript": "6.0.0-dev.20251230",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "license": "MIT",
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/philjs/philjs.git",
    "directory": "packages/philjs-wasm"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme",
  "keywords": [
    "philjs",
    "typescript",
    "framework",
    "wasm",
    "webassembly",
    "rust",
    "wasm-bindgen",
    "signals",
    "reactivity",
    "component",
    "codegen",
    "bindings"
  ],
  "engines": {
    "node": ">=24"
  }
}
