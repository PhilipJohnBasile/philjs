{
  "name": "philjs-wasm",
  "version": "2.0.0",
  "description": "Rust/WebAssembly integration for PhilJS with reactive signals",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite-plugin.d.ts",
      "import": "./dist/vite-plugin.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "echo 'Lint philjs-wasm'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "peerDependencies": {
    "philjs-core": "^2.0.0",
    "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "peerDependenciesMeta": {
    "philjs-core": {
      "optional": false
    },
    "vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "philjs-core": "workspace:*",
    "rollup": "^4.28.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  },
  "license": "MIT",
  "author": "PhilJS Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/philjs.git",
    "directory": "packages/philjs-wasm"
  },
  "bugs": {
    "url": "https://github.com/yourusername/philjs/issues"
  },
  "homepage": "https://github.com/yourusername/philjs#readme",
  "keywords": [
    "philjs",
    "wasm",
    "webassembly",
    "rust",
    "wasm-bindgen",
    "signals",
    "reactivity",
    "component"
  ]
}
