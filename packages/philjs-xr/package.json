{
  "name": "@philjs/xr",
  "version": "0.1.0",
  "type": "module",
  "description": "WebXR Components - VR/AR/MR experiences with hand tracking, spatial UI, gestures, and 3D reactive primitives",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --watch",
    "test": "vitest"
  },
  "keywords": [
    "philjs",
    "webxr",
    "vr",
    "ar",
    "mr",
    "xr",
    "virtual-reality",
    "augmented-reality",
    "hand-tracking",
    "spatial-ui",
    "gestures",
    "immersive"
  ],
  "author": "PhilJS Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.0",
    "@types/webxr": "^0.5.10",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=24"
  }
}
